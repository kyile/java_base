<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>


<VBox spacing="10.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8">
   <children>
      <ImageView fitWidth="300.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@imgs/javafx.jpg" />
         </image>
      </ImageView>
      <HBox alignment="CENTER" spacing="10.0" VBox.vgrow="ALWAYS">
         <children>
            <Button mnemonicParsing="false" text="이전" />
            <Button mnemonicParsing="false" text="다음" HBox.hgrow="ALWAYS" >
            <maxWidth><Double fx:constant="MAX_VALUE"/></maxWidth>
            <!-- maxWidth="1.7976931348623157E308" -->
            </Button>
         </children>
      </HBox>
   </children>
</VBox>

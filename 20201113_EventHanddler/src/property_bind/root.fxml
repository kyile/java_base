<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<!--root.fxml  -->

<VBox 
fx:controller="property_bind.RootController"
prefHeight="200.0" prefWidth="300.0" spacing="10.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8">
   <children>
      <TextArea fx:id="txtArea1" prefHeight="200.0" prefWidth="200.0" promptText="txtArea1">
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </TextArea>
      <TextArea fx:id="txtArea2" prefHeight="200.0" prefWidth="200.0" promptText="txtArea2" />
   </children>
</VBox>
